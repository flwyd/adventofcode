import_file("../.iex.exs")
c("day<%= day_num %>.exs")
daymod = Day<%= day_num %>
exampletxt = "input.example.txt"
actualtxt = "input.actual.txt"
IO.puts("Run on #{exampletxt}: Runner.run(Day<%= day_num %>, exampletxt) or Today.run_example")
IO.puts("Run on #{actualtxt}: Runner.run(Day<%= day_num %>, actualtxt) or Today.run_actual")

defmodule Today do
  use InteractiveDay, Day<%= day_num %>
end
